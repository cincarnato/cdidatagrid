{
    "name": "cdi/cdidatagrid",
    "description": "Zend Framework 2 Module that provides a datagrid",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ZfcDatagrid",
        "datagrid",
        "datatable",
        "table",
        "paginator",
        "module",
        "zf",
        "zf2",
        "zf3",
        "Zend Framework",
        "Zend Framework 2",
        "Zend Framework 3",
        "zend",
        "doctrine",
        "doctrine 2",
        "order",
        "filter",
        "pager",
        "add",
        "edit",
        "delete",
        "view",
        "abm",
        "crud",
        "php"
    ],
    "authors": [{
            "name": "Cristian Incarnato",
            "email": "cristian.cdi@gmail.com"
        }],
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 5000
    },
    "require": {
        "php": ">=5.6",
         "zendframework/zend-mvc": "^3.0.1",
        "doctrine/doctrine-orm-module": "^1.0",
        "zendframework/zend-view": "~2.2",
        "zendframework/zend-http": "~2.2",
        "zendframework/zend-paginator": "~2.2",
         "zendframework/zend-code": "^2.6 || ^3.0"
    },
    "suggest": {
        "doctrine/doctrine-orm-module": ">=0.9"
    },
    "autoload": {
        "psr-4": {
            "CdiDataGrid\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
